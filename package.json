{
  "name": "tech-cat",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "concurrently -c bgGreen,bgBlue -n TURBO,DOCKER --kill-others \"turbo run dev --parallel\" \"pnpm --prefix firebase run docker:start\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx}\""
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "plop": "^3.0.5",
    "prettier": "2.6.2",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.4"
}